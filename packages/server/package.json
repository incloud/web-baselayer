{
  "name": "@baselayer/server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn clean; tsc -b ../common .",
    "start": "yarn clean; tsc-watch --outDir ./dist --onSuccess 'node --inspect=0.0.0.0:9229 ./dist/index.js'",
    "lint": "tslint --project .",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "fixtures": "ts-node ./node_modules/typeorm-fixtures-cli/dist/cli.js ./src/fixture --config ormconfig.js --sync",
    "test": "NODE_ENV=test jest --verbose --coverage --forceExit --runInBand",
    "test:ci": "NODE_ENV=test jest --verbose --coverage --ci --forceExit --detectOpenHandles --runInBand",
    "clean": "rimraf ./dist"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/ejs": "^2.6.3",
    "@types/express-jwt": "^0.0.41",
    "@types/graphql": "^14.0.7",
    "@types/ioredis": "^4.0.6",
    "@types/jest": "^24.0.1",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/nodemailer": "^4.6.6",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/uuid": "^3.4.4",
    "apollo-server-testing": "^2.4.8",
    "deepmerge": "^3.2.0",
    "jest": "23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "tsc-watch": "^2.1.2",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typeorm-fixtures-cli": "^1.1.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@baselayer/common": "1.0.0",
    "apollo-server-express": "^2.4.8",
    "argon2": "^0.20.1",
    "body-parser": "^1.18.3",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "dataloader": "^1.4.0",
    "dotenv-safe": "^6.1.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "generate-password": "^1.4.1",
    "graphql": "^14.1.1",
    "ioredis": "^4.6.2",
    "jsonwebtoken": "^8.4.0",
    "jwks-rsa": "^1.4.0",
    "nodemailer": "^5.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.3",
    "type-graphql": "^0.17.0",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.12",
    "typeorm-typedi-extensions": "^0.2.3",
    "uuid": "^3.3.2"
  },
  "workspaces": {
    "nohoist": [
      "typeorm",
      "typeorm/**",
      "typeorm-fixtures-cli",
      "typeorm-fixtures-cli/**/"
    ]
  }
}
