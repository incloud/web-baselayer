  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Docker Attach",
      "port": 9229,
      "restart": true,
      "sourceMaps": true,
      "internalConsoleOptions": "neverOpen",
      "cwd": "${workspaceFolder}/packages/server",
      "localRoot": "${workspaceFolder}/packages/server",
      "remoteRoot": "/var/app/packages/server",
      "outFiles": ["${workspaceFolder}/app/server/dist/src/**/*.js"]
    }
  ]
}
